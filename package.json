{
  "name": "nodejs-cli-experiment",
  "description": "node attempt at an interactive cli application",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "bin": {
    "nodejs-cli-experiment": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.js --exec \"yarn run fix --cache && babel-node\"",
    "build": "set NODE_ENV=production && rimraf dist && babel src -d dist",
    "format": "prettier --write src -q src/**/*.js",
    "lint": "eslint src --ext .js,.jsx",
    "fix": "eslint src --ext .js,.jsx --fix",
    "precommit": "lint-staged",
    "prepublish": "yarn fix && yarn format && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jwoo92/node-interactive-cli-experiment.git"
  },
  "keywords": [
    "github",
    "search",
    "node",
    "cli"
  ],
  "author": {
    "name": "Justin Woodward",
    "email": "justin@justinwoodward.com",
    "url": "http://justinwoodward.com"
  },
  "bugs": {
    "url": "https://github.com/jwoo92/node-interactive-cli-experiment/issues"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "figlet": "^1.2.0",
    "inquirer": "^6.1.0",
    "shelljs": "^0.8.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-closure-elimination": "^1.3.0",
    "babel-plugin-loop-optimizer": "^1.4.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.9.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.0.5",
    "nodemon": "^1.17.3",
    "prettier": "^1.12.1",
    "rimraf": "^2.6.2"
  },
  "lint-staged": {
    "src/**/*.js": [
      "yarn fix",
      "yarn build",
      "git add"
    ]
  }
}
